{
  "name": "is-file-equal",
  "version": "1.3.0",
  "description": "Check file equals",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "mocha -r ts-node/register/transpile-only test/**/*.spec.ts",
    "build": "npm run clean && rollup -c && tsc --build --clean",
    "clean": "rimraf dist",
    "format": "prettier --write **/*.{ts,js,jsx,tsx,json,css,md}",
    "preversion": "npm run test && npm run build",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zharkov-eu/is-equal.git"
  },
  "keywords": [
    "file",
    "equal"
  ],
  "author": "Evgenii Zharkov <zharkov.ev.u@yandex.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zharkov-eu/is-equal/issues"
  },
  "homepage": "https://github.com/zharkov-eu/is-equal#readme",
  "devDependencies": {
    "@rollup/plugin-typescript": "4.1.1",
    "@types/mocha": "7.0.2",
    "@types/node": "12.12.37",
    "mocha": "7.1.2",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "rollup": "2.7.2",
    "rollup-plugin-dts": "1.4.0",
    "ts-node": "8.9.1",
    "tslib": "1.11.1",
    "typescript": "3.8.3"
  }
