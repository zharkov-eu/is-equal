{
  "name": "is-file-equal",
  "version": "1.3.0",
  "description": "Check file equals",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && cross-env NODE_ENV=production rollup -c",
    "test": "cross-env mocha -r ts-node/register test/**/*.spec.ts",
    "release": "npm run build && cross-vars git commit -am $npm_package_version && cross-vars git tag $npm_package_version && git push && git push --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zharkov-eu/is-equal.git"
  },
  "keywords": [
    "file",
    "equal"
  ],
  "author": "Evgeni Zharkov <zharkov.ev.u@yandex.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zharkov-eu/is-equal/issues"
  },
  "homepage": "https://github.com/zharkov-eu/is-equal#readme",
  "devDependencies": {
    "@types/mocha": "^5.2.6",
    "cross-env": "^5.2.0",
    "cross-vars": "^1.1.0",
    "mocha": "^6.1.4",
    "rollup": "^1.12.3",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-dts": "^0.15.1",
    "ts-node": "^8.1.0",
    "tslib": "^1.9.3",
    "typescript": "^3.4.5"
  }
}
